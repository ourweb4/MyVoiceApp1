<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyVoiceApp.UserAPI.Pages.RegistrarPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="txtusername" Placeholder="Username"></Entry>
            <Entry x:Name="txtpassword" Placeholder="Password"></Entry>
            <Entry x:Name="txtemail" Placeholder="Email"></Entry>
            <Entry x:Name="txtfirst" Placeholder="First"></Entry>
            <Entry x:Name="txtlast" Placeholder="Last"></Entry>
            <Entry x:Name="txtaddress" Placeholder="Address"></Entry>

            <Entry x:Name="txtcity" Placeholder="City"></Entry>
            <Entry x:Name="txtstate" Placeholder="State"></Entry>
            <Entry x:Name="txtzip" Placeholder="Zip"></Entry>
            <Entry x:Name="txtphone" Placeholder="Phone"></Entry>

            <Button Text="Registrar" Clicked="Butreg"></Button>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>